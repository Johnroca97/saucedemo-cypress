{
  "name": "saucedemo-cypress-typescript",
  "version": "1.0.0",
  "description": "SauceDemo automation tests with Cypress and TypeScript - converted from Playwright",
  "main": "index.js",
  "scripts": {
    "test:open": "npx cypress open",
    "test:run": "npx cypress run",
    "test:chrome": "npx cypress run --browser chrome",
    "test:firefox": "npx cypress run --browser firefox",
    "test:edge": "npx cypress run --browser edge",
    "test:headed": "npx cypress run --headed",
    "test:spec": "npx cypress run --spec",
    "clean": "rm -rf cypress/screenshots cypress/videos cypress/reports",
    "lint": "eslint cypress/**/*.ts",
    "type-check": "tsc --noEmit",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "report": "npx mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json && npx marge cypress/reports/report.json --reportDir cypress/reports/html --inline"
  },
  "dependencies": {
    "cypress": "^15.0.0"
  },
  "devDependencies": {
    "@cypress/eslint-plugin-dev": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "cypress",
    "typescript",
    "automation",
    "testing",
    "saucedemo",
    "e2e",
    "playwright-conversion",
    "page-object-model"
  ],
  "author": "Converted from Playwright to Cypress",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tu-usuario/saucedemo-cypress.git"
  },
  "homepage": "https://github.com/tu-usuario/saucedemo-cypress#readme"
}
